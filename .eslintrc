{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base/legacy"
  ],
  "globals": {
    "wx": true,
    "Float32Array": true,
    "Int16Array": true,
    "Uint8ClampedArray": true,
    "Proxy": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-continue": 0,
    "no-multi-assign": 0,
    "max-len": 0,
    "no-mixed-operators": "off",
    "no-plusplus": 0,
    "consistent-return": 0,
    "func-names": 0,
    "no-nested-ternary": 0,
    "no-bitwise": 0,
    "no-restricted-globals": 0
  }
}
